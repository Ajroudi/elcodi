{
    "name": "elcodi/elcodi",
    "description": "Barebones Ecommerce functionality for Symfony",
    "keywords": ["elcodi", "symfony", "ecommerce", "bundle"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Aldo Chiecchia",
            "email": "aldo.chiecchia@befactory.es"
        },
        {
            "name": "Marc Morera",
            "email": "marc.morera@befactory.es"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://github.com/elcodi/elcodi/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.4",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "doctrine/doctrine-cache-bundle": "~1.0",
        "symfony/yaml": "~2.4",
        "doctrine/doctrine-fixtures-bundle": "v2.2.0",
        "stof/doctrine-extensions-bundle": "~1.1@dev",
        "symfony/expression-language": "~2.4",
        "knplabs/knp-gaufrette-bundle": "~0.1"
    },
    "require-dev": {
        "doctrine/data-fixtures": "v1.0.0",
        "phpunit/phpunit": ">=4.0.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "autoload": {
        "psr-4": {
            "Elcodi\\": "src/Elcodi"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}

